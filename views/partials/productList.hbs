{{#each products}}
    <!-- Product starts -->

    <div class="col-md-4">
        <div class="product-wrapper">
            <div class="product-image"><a href="product-overview?id={{_id}}"><img src="{{imagePath}}" height="auto"></a></div>
            <div class="product-title"><b>{{title}}</b></div>
            <div class="product-artist">{{artist}}</div>
            <div class="product-price">${{price}} CAD</div>
        </div>
    </div>

    <!-- Product ends -->
{{/each}}
